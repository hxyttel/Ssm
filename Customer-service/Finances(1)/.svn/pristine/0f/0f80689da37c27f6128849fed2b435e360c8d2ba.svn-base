package com.p2p.pojo;

import com.baomidou.mybatisplus.annotations.TableField;

/**
 * 联系表
 * 曾林
 * 2017-11-14
 * */
public class Contact {
	@TableField("c_id")
	private	Integer cid;//主键ID
	@TableField("c_ephone")
	private String cephone;//企业电话
	@TableField("c_sphone")
	private String csphone;//客服电话
	@TableField("c_wechartimgurl")
	private String cwechartimgurl;//微信图url
	@TableField("c_weboimgurl")
	private String cweboimgurl;//微博图url
	@TableField("c_qqnum")
	private String cqqnum;//qq号
	@TableField("c_address")
	private String caddress;//微博号
	@TableField("c_xs")
	private Integer cxs;//是否前台显示 0 不显示        1显示
	public Contact(Integer cid, String cephone, String csphone, String cwechartimgurl, String cweboimgurl,
			String cqqnum, String caddress, Integer cxs) {
		super();
		this.cid = cid;
		this.cephone = cephone;
		this.csphone = csphone;
		this.cwechartimgurl = cwechartimgurl;
		this.cweboimgurl = cweboimgurl;
		this.cqqnum = cqqnum;
		this.caddress = caddress;
		this.cxs = cxs;
	}
	public Contact() {
		super();
	}
	@Override
	public String toString() {
		return "Contact [cid=" + cid + ", cephone=" + cephone + ", csphone=" + csphone + ", cwechartimgurl="
				+ cwechartimgurl + ", cweboimgurl=" + cweboimgurl + ", cqqnum=" + cqqnum + ", caddress=" + caddress
				+ ", cxs=" + cxs + "]";
	}
	public Integer getCid() {
		return cid;
	}
	public void setCid(Integer cid) {
		this.cid = cid;
	}
	public String getCephone() {
		return cephone;
	}
	public void setCephone(String cephone) {
		this.cephone = cephone;
	}
	public String getCsphone() {
		return csphone;
	}
	public void setCsphone(String csphone) {
		this.csphone = csphone;
	}
	public String getCwechartimgurl() {
		return cwechartimgurl;
	}
	public void setCwechartimgurl(String cwechartimgurl) {
		this.cwechartimgurl = cwechartimgurl;
	}
	public String getCweboimgurl() {
		return cweboimgurl;
	}
	public void setCweboimgurl(String cweboimgurl) {
		this.cweboimgurl = cweboimgurl;
	}
	public String getCqqnum() {
		return cqqnum;
	}
	public void setCqqnum(String cqqnum) {
		this.cqqnum = cqqnum;
	}
	public String getCaddress() {
		return caddress;
	}
	public void setCaddress(String caddress) {
		this.caddress = caddress;
	}
	public Integer getCxs() {
		return cxs;
	}
	public void setCxs(Integer cxs) {
		this.cxs = cxs;
	}
	
}
