/*公共弹框*/
.dialog {
	display: none;
	position: fixed;
	width: 480px;
	top: 20%;
	left: 50%;
	margin-left: -240px;
	z-index: 1000;
	background-color: #fff;
	border-radius: 8px;
}
.dialog .dialog-header {
	position: relative;
	text-align: left;
	height: 70px;
	line-height: 70px;
	border-bottom: 1px solid #e5e5e5;
}

.dialog .dialog-header .header-title {
	padding-left: 12px;
	line-height: 70px;
	font-size: 20px;
	color: #444;
}
.dialog .dialog-header .header-title .icon {
	float: left;
	margin: 20px 10px 20px;
	width: 30px;
	height: 30px;
	background: url("../images/sprite_1.png") no-repeat;
}
.dialog .dialog-header .header-title span {
	float: left;
}
.dialog .dialog-header .header-title .tip {
	background-position: -50px 0;
}
.dialog .dialog-header .header-title .ok {
	background-position: -20px 0;
}
.dialog .dialog-header .header-title .sign {
	background-position: -111px 0;
}
.dialog .dialog-header .header-title .info {
	background-position: -296px 0;
}
.dialog .dialog-header .close {
	position: absolute;
	right: 20px;
	top: 26px;
	width: 20px;
	height: 20px;
	background: url("../images/sprite_1.png") no-repeat 0 0;
	/*background-position: 0 0;*/
	cursor: pointer;
	transition: all 300ms ease;
}
.dialog .dialog-header .close:hover {
	-webkit-transform: rotate(180deg);
	-moz-transform: rotate(180deg);
	-ms-transform: rotate(180deg);
	-o-transform: rotate(180deg);
	transform: rotate(180deg);
}
.dialog .dialog-body {
	padding: 22px 20px;
	font-size: 14px;
	color: #666;
}
.dialog .dialog-body .content {
	line-height:26px;
	text-align: center;
}
.dialog .dialog-footer {
	padding: 0 20px 20px;
	text-align: center;
}
