/*----msgBox-----*/
.m2-msgBox{
	position: relative;
	height: 26px;
	border: 1px solid #36BEAC;
	background-color: #fff;
	margin-bottom: 10px;
}
.m2-msgBox b{
	position: relative;
	display: inline-block;
	padding-left: 18px;
	padding-right: 25px;
	cursor: default;
	line-height: 26px;
	background-color: #36BEAC;
	color: #fff;
}
.m2-msgBox b em{
	position: absolute;
	display: block;
	width: 0;
	height: 0;
	right: 0px;
	top: 0px;
	border-width: 13px;
	border-style: solid;
	border-color: transparent #fff transparent transparent;
}
.m2-msgBox a{
	color: #33beac;
}
.m2-msgBox span{
	padding-left: 12px;
}
.m2-msgBox a:link{color: #33beac;}
.m2-msgBox a:visited{color: #33beac;}
.m2-msgBox a:hover{color: #53decc;}
.m2-msgBox a:active{color: #53decc;}

.m2-msgBox-close{
	display: inline-block;
	position: absolute;
	right: 22px;
	top: 6px;
	width: 14px;
	height: 14px;
	background: url(../images/m2-userCenter-icon.png) no-repeat -20px -20px;
	cursor: pointer;
}

