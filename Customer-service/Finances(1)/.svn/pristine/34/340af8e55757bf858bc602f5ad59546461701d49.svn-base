		  		<if test="fstatus!=null">and f_status=#{fstatus}</if>
		  		<if test="fsecuritymea!=null">and f_securitymea=#{fsecuritymea}</if>
		  		<if test="fsecurity!=null">and f_security=#{fsecurity}</if>
