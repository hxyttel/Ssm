package com.p2p.pojo;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 保存页面需要获取的字段
 * 
 * @author lxj
 * */
@SuppressWarnings("serial")
public class RepayAllInfo implements Serializable{
	private BigDecimal allmongy;  //还款总金额
	private BigDecimal bidMoney;  //借款金额
	private BigDecimal allInterest;  //总利息
	private BigDecimal allrate;  //期利率
	private Integer rastyle;  //还款方式
	private Integer raterm;  //还款期限
	private BigDecimal termmoney;  //每期偿还
	private Integer termcount;  //已还期限
	private Double Income;  //平台每期收益
	
	public RepayAllInfo() {
		super();
	}
	
	public RepayAllInfo(BigDecimal allmongy, BigDecimal bidMoney, BigDecimal allInterest, BigDecimal allrate,
			Integer rastyle, Integer raterm, BigDecimal termmoney, Integer termcount,Double Income) {
		super();
		this.allmongy = allmongy;
		this.bidMoney = bidMoney;
		this.allInterest = allInterest;
		this.allrate = allrate;
		this.rastyle = rastyle;
		this.raterm = raterm;
		this.termmoney = termmoney;
		this.termcount = termcount;
		this.Income = Income;
	}

	@Override
	public String toString() {
		return "RepayAllInfo [allmongy=" + allmongy + ", bidMoney=" + bidMoney + ", allInterest=" + allInterest
				+ ", allrate=" + allrate + ", rastyle=" + rastyle + ", raterm=" + raterm + ", termmoney=" + termmoney
				+ ", termcount=" + termcount + ", Income=" + Income + "]";
	}

	public BigDecimal getAllmongy() {
		return allmongy;
	}

	public void setAllmongy(BigDecimal allmongy) {
		this.allmongy = allmongy;
	}

	public BigDecimal getBidMoney() {
		return bidMoney;
	}

	public void setBidMoney(BigDecimal bidMoney) {
		this.bidMoney = bidMoney;
	}

	public BigDecimal getAllInterest() {
		return allInterest;
	}

	public void setAllInterest(BigDecimal allInterest) {
		this.allInterest = allInterest;
	}

	public BigDecimal getAllrate() {
		return allrate;
	}

	public void setAllrate(BigDecimal allrate) {
		this.allrate = allrate;
	}

	public Integer getRastyle() {
		return rastyle;
	}

	public void setRastyle(Integer rastyle) {
		this.rastyle = rastyle;
	}

	public Integer getRaterm() {
		return raterm;
	}

	public void setRaterm(Integer raterm) {
		this.raterm = raterm;
	}

	public BigDecimal getTermmoney() {
		return termmoney;
	}

	public void setTermmoney(BigDecimal termmoney) {
		this.termmoney = termmoney;
	}

	public Integer getTermcount() {
		return termcount;
	}

	public void setTermcount(Integer termcount) {
		this.termcount = termcount;
	}

	public Double getIncome() {
		return Income;
	}

	public void setIncome(Double income) {
		Income = income;
	}
	
}
