.m2-commonNav .selected {color: #fff; background-color: #F34B43;} 
#fixbar{width:48px; position:fixed; top:0; right:0px; background:#ff6666; z-index:1000; bottom:0;}
#fixbar .fixbox_bar {position: fixed;right: -318px;top: 0;bottom: 0;width: 318px;background-color: #333333;color: white;overflow-Y:scroll;}
#fixbar .fixbox_bar #fixnavbar_one {color: white;font-size: 16px;}
#fixbar .fixbox_bar #fixnavbar_one .sumtop {margin: 10px 20px;}
#fixbar .fixbox_bar #fixnavbar_one .sumtop .top {text-align: center;margin-bottom: 10px;}
#fixbar .fixbox_bar #fixnavbar_one .sumtop .bottom div {float: right;text-align: center;line-height: 38px;padding-right: 35px;}
#fixbar .fixbox_bar #fixnavbar_one .sumtop .bottom button {width: 90px;height: 32px;background-color: #ff6666;font-size: 18px;border: 0;border-radius: 12px;color: white;}
#fixbar .fixbox_bar #fixnavbar_one .summiddle {margin: 20px;}
#fixbar .fixbox_bar #fixnavbar_one .summiddle ul li {display: inline-block;height: 80px;width: 132px;background-color: #515151;text-align: center;font-size: 18px;margin-left: -4px;padding-top: 4px;}
#fixbar .fixbox_bar #fixnavbar_one .summiddle ul li p {color: #ff6666;line-height: 40px;font-weight: normal;font-size: 16px;}
#fixbar .fixbox_bar #fixnavbar_one .summiddle ul li span {font-weight: normal;font-size: 16px;}
#fixbar .fixbox_bar #fixnavbar_one .summiddle .content {background-color: #515151;text-align: center;margin: 2px 0 2px -4px;line-height: 34px;padding: 20px;}
#fixbar .fixbox_bar #fixnavbar_one .summiddle .lastcontent{background-color: #515151;text-align: center;margin: 2px 0 2px -4px;line-height: 34px;padding: 20px;border-bottom-left-radius: 10px;border-bottom-right-radius: 10px;}
#fixbar .fixbox_bar .sumbottom {margin: 20px;border: 0;border-radius: 10px;background-color: #515151;text-align: center;line-height: 50px;font-size: 16px;}
#fixbar .fixbox_bar .sumbottom:hover{cursor: pointer;}

#fixbar .fixbox_bar .common .sumtop {margin:18px;text-align: center;font-size: 20px;position:relative;}
#fixbar .fixbox_bar .common .sumtop .right_jiantou {position:absolute;top:0;left:0;}
#fixbar .fixbox_bar .common .summiddle {margin: 20px;}
#fixbar .fixbox_bar .common .summiddle ul li {display: inline-block;height: 76px;width: 130px;background-color: #515151;text-align: center;font-size: 14px;margin-left: -2px;padding-top: 10px;}
#fixbar .fixbox_bar .common .summiddle ul li p {color: #ff6666;line-height: 40px;font-weight: normal;font-size: 24px;}
#fixbar .fixbox_bar .common .red_pack {background-color: white;margin: 20px;padding-top: 6px;position: relative;}
#fixbar .fixbox_bar .common .red_pack .left {background-image: url(../../newcommon/images/hongbao.png);width: 98px;height: 64px;text-align: center;color: white;margin: 20px;font-size: 12px;line-height: 33px;padding-top: 6px;}
#fixbar .fixbox_bar .common .red_pack .right {padding: 0 10px;position: absolute;top: 26px;right: 0;border-left: 1px dashed #ebebeb;color: #ff3333;font-size: 16px;line-height: 24px;width: 110px;}
#fixbar .fixbox_bar .common .red_pack .right .spright {color: #666666;}
#fixbar .fixbox_bar .common .red_pack .bottom {background-color: #f0f0f0;text-align: center;color: #9f9f9f;line-height: 30px;border-top: 1px dashed #d7d7d7;}

#fixbar .fixbox_bar #fixnavbar_four .sumtop {margin:18px;text-align: center;font-size: 20px;}
#fixbar .fixbox_bar #fixnavbar_four .website_news .news_box {background-color: #515151;margin: 20px 20px 2px 20px;border-top-left-radius: 10px;border-top-right-radius: 10px;color: white;font-size: 14px;}
#fixbar .fixbox_bar #fixnavbar_four .website_news .news_box p{color: #66cbff;padding: 10px 16px;}
#fixbar .fixbox_bar #fixnavbar_four .website_news .news_box .right {float: right;}
#fixbar .fixbox_bar #fixnavbar_four .website_news .news_box .content {text-align: justify;margin: 0 12px;padding-bottom: 4px;}
#fixbar .fixbox_bar #fixnavbar_four .website_news .bottom {background-color: #515151;margin: 2px 20px;border-bottom-left-radius: 10px;border-bottom-right-radius: 10px;color: white;font-size: 14px;line-height: 34px;padding-left: 18px;}
#fixbar .fixbox_bar #fixnavbar_four .website_news .bottom .right {float: right;padding-right: 16px;}

