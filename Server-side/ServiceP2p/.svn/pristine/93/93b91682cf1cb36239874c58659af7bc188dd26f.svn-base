package com.p2p.pojo;

import java.io.Serializable;

import com.baomidou.mybatisplus.annotations.TableField;

public class Admin implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	@TableField("a_id")
	private Integer aid;//编号
	
	@TableField("a_password")
	private String apassword;//登入密码
	
	@TableField("a_name")
	private String aname;//姓名
	
	@TableField("a_card")
	private String acard;//身份证
	
	@TableField("a_phone")
	private String aphone;//手机号码
	
	@TableField("a_mail")
	private String amail;//邮箱
	
	@TableField("a_img")
	private String aimg;//头像

	public Integer getAid() {
		return aid;
	}

	public void setAid(Integer aid) {
		this.aid = aid;
	}

	public String getApassword() {
		return apassword;
	}

	public void setApassword(String apassword) {
		this.apassword = apassword;
	}

	public String getAname() {
		return aname;
	}

	public void setAname(String aname) {
		this.aname = aname;
	}

	public String getAcard() {
		return acard;
	}

	public void setAcard(String acard) {
		this.acard = acard;
	}

	public String getAphone() {
		return aphone;
	}

	public void setAphone(String aphone) {
		this.aphone = aphone;
	}

	public String getAmail() {
		return amail;
	}

	public void setAmail(String amail) {
		this.amail = amail;
	}

	public String getAimg() {
		return aimg;
	}

	public void setAimg(String aimg) {
		this.aimg = aimg;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}

	public Admin(Integer aid, String apassword, String aname, String acard, String aphone, String amail, String aimg) {
		super();
		this.aid = aid;
		this.apassword = apassword;
		this.aname = aname;
		this.acard = acard;
		this.aphone = aphone;
		this.amail = amail;
		this.aimg = aimg;
	}

	public Admin() {
		super();
	}

	@Override
	public String toString() {
		return "Admin [aid=" + aid + ", apassword=" + apassword + ", aname=" + aname + ", acard=" + acard + ", aphone="
				+ aphone + ", amail=" + amail + ", aimg=" + aimg + "]";
	}

	
	
	
}
